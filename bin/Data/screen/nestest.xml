<?xml version="1.0" encoding="UTF-8"?>
<screen id="nestest">
  <panel x="200" y="100">
    <pair spacing="50">
      <text text="TEST" font="glitch" />
      <button texture="buttons">
        <skin type="normal" left="0" top="0" width="16" height="16" />
        <skin type="hover" left="0" top="16" width="16" height="16" />
      </button>
    </pair>
    <pair spacing="50">
      <text text="TESTT" font="glitch" />
      <button text="OFF" activetext="ON" type="toggle" font="glitch" />
    </pair>
    <pair spacing="50">
      <text text="TESTS" font="glitch" />
      <button text="S1" type="setting" font="glitch">
        <setting text="S2" />
        <setting text="S3" />
      </button>
    </pair>
  </panel>
</screen>
